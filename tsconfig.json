{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "CommonJS",
    "declaration": false,
    "sourceMap": true,
    "outDir": "lib",
    "strict": true,
    "moduleResolution": "nodenext",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "esModuleInterop": true
  },
  "include": ["src"]
}
